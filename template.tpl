  const myLocation = getUrl();
  
  __easyMetaTagSqudata({
        'url' 		 : myLocation, 
        'support'    : _support,
        'page'       : _page,
        'category'   : _category,
        'category_id': _category_id,
        'prd'        : _prd,
        'm'          : _m,
        'sha'        : _sha,
        'cart_prd'   : _cart_prd,
        'cart_id'    : _cart_prd,
        'lamount'    : _lamount,
        });

  data.gtmOnSuccess();
};

const __easyMetaTagSqudata = createArgumentsQueue('__easyMetaTagSqudata', '__easyMetaTagSqudata.q');

const urlSrc = 'https://asset.easydmp.net/mt/squadata/mt.js';
if (queryPermission('inject_script', urlSrc)) {
  injectScript(urlSrc, onSuccess(), data.gtmOnFailure());
} else {
  log('Squadata: Script load failed due to permissions mismatch.');
  data.gtmOnFailure();
}


